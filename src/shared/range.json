{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "https://raw.githubusercontent.com/SPGoding/datapack-json/master/src/shared/range.json",
    "oneOf": [
        {
            "type": "number"
        },
        {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "min": {
                    "type": "number"
                },
                "max": {
                    "type": "number"
                }
            }
        }
    ]
}
